{
	"name": "pu-learning-sqlite-db",
	"version": "1.0.0",
	"description": "SQLite database for PU Learning experiment workbench",
	"main": "index.js",
	"scripts": {
		"db:generate": "prisma generate --schema=./prisma/schema.prisma",
		"db:push": "prisma db push --schema=./prisma/schema.prisma",
		"db:studio": "prisma studio --schema=./prisma/schema.prisma",
		"db:reset": "rm -f ./prisma/pu_practice.db && prisma db push --schema=./prisma/schema.prisma",
		"db:migrate:init": "prisma migrate dev --name init --schema=./prisma/schema.prisma",
		"db:seed": "node scripts/seed.js",
		"db:import": "python3 scripts/import-room-samples-v2.py",
		"db:validate": "node scripts/validate-data.js",
		"start": "node server.js",
		"dev": "nodemon server.js"
	},
	"dependencies": {
		"@prisma/client": "^5.7.0",
		"express": "^4.18.2",
		"cors": "^2.8.5",
		"csv-parser": "^3.0.0",
		"fast-csv": "^4.3.6"
	},
	"devDependencies": {
		"prisma": "^5.7.0",
		"nodemon": "^3.0.2"
	}
}
